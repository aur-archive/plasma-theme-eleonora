pkgname=plasma-theme-eleonora
pkgver=1.1
pkgrel=2
pkgdesc="A milky theme based on Glassified and Aya."
arch=('i686' 'x86_64')
url="http://www.kde-look.org/content/show.php/Eleonora?content=98341"
license=('GPL')
depends=('kdebase-workspace')
source=(http://www.kde-look.org/CONTENT/content-files/98341-Eleonora.tar.gz)
md5sums=('161662ec066dffb52a6ba1872b7cb7a6')

build() {

mkdir -p $pkgdir/usr/share/apps/desktoptheme
cd $srcdir
cp -r Eleonora $pkgdir/usr/share/apps/desktoptheme/

}
